<?xml version="1.0" encoding="UTF-8"?>

<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <modules>
        <module>service-sample-api</module>
        <module>service-sample</module>
    </modules>

    <parent>
        <groupId>com.fresh</groupId>
        <artifactId>fresh-common-dependencies</artifactId>
        <version>1.0.0</version>
        <!--<relativePath/>-->
    </parent>
    <groupId>com.fresh.xy</groupId>
    <artifactId>xy-service-sample</artifactId>
    <version>1.0.0</version>
    <packaging>pom</packaging>
    <name>xy-service-sample</name>
    <description>xy-service-sample</description>
    <url>https://www.github.com/freshlml/xy-service-sample</url>
    <scm>
        <url>https://www.github.com/freshlml/xy-service-sample</url>
    </scm>

    <properties>
        <!--当xy-common版本号变更时，此处也发生变更 -->
        <xy-common.version>1.0.0</xy-common.version>
        <!-- 当版本好变更时，此处也发生变更 -->
        <xy-mybatisplus-starter.version>1.0.0</xy-mybatisplus-starter.version>
        <xy-redis-starter.version>1.0.0</xy-redis-starter.version>
        <xy-rmq-starter.version>1.0.0</xy-rmq-starter.version>

        <!-- 当前版本号1.0.0，如果1.0.0稳定后，继续开发1.0.1，则将版本号改成1.0.1 -->
        <service-sample-api.version>1.0.0</service-sample-api.version>
        <service-sample.version>1.0.0</service-sample.version>

        <!-- 当版本好变更时，此处也发生变更 -->
        <service-sample2-api.version>1.0.0</service-sample2-api.version>
    </properties>

    <dependencyManagement>
        <dependencies>
            <!-- xy-common -->
            <dependency>
                <groupId>com.fresh.xy</groupId>
                <artifactId>xy-common</artifactId>
                <version>${xy-common.version}</version>
            </dependency>
            <!-- mybatisplus-starter -->
            <dependency>
                <groupId>com.fresh.xy</groupId>
                <artifactId>xy-mybatisplus-starter</artifactId>
                <version>${xy-mybatisplus-starter.version}</version>
            </dependency>
            <!-- redis-starter -->
            <dependency>
                <groupId>com.fresh.xy</groupId>
                <artifactId>xy-redis-starter</artifactId>
                <version>${xy-redis-starter.version}</version>
            </dependency>
            <!-- rmq-starter -->
            <dependency>
                <groupId>com.fresh.xy</groupId>
                <artifactId>xy-rmq-starter</artifactId>
                <version>${xy-rmq-starter.version}</version>
            </dependency>

            <!-- service-sample-api -->
            <dependency>
                <groupId>com.fresh.xy</groupId>
                <artifactId>service-sample-api</artifactId>
                <version>${service-sample-api.version}</version>
            </dependency>
            <!-- service-sample2.api -->
            <dependency>
                <groupId>com.fresh.xy</groupId>
                <artifactId>service-sample2-api</artifactId>
                <version>${service-sample2-api.version}</version>
            </dependency>
        </dependencies>
    </dependencyManagement>

</project>
